generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/@prisma/client-auth"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_AUTH_URL") 
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  name      String                 
  password  String
  createdAt DateTime @default(now()) 
  updatedAt DateTime @updatedAt         
}